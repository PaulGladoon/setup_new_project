    "chai": "^4.2.0",
    "log4js": "^6.1.0",
    "minimist": "^1.2.0",
    "mocha": "^7.0.0",
    "mocha-allure-reporter": "^1.4.0",
    "node-fetch": "^2.6.0",
    "protractor": "^5.4.2",
    "protractor-retry": "^1.2.9",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  }
